{
    "name": "syn",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["style.css"],
            "js": ["content.js", "box.js", "action.js", "synonym.js", "meaning.js" ,"selection.js"]
        }
    ],
    "web_accessible_resources": ["copypaste.svg"],
    "browser_action":
    {
        "default_popup": "popup.html",
        "default_title": "Syn"
    },
    "background":{
        "service_worker": "background.js"
    },
    "commands":{
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "Ctrl+Shift+F"
            },
            "description": "Translate"
        }
    }
}